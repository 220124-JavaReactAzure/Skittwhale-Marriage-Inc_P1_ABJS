<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">

	<display-name>MARRIAGE!</display-name>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.revature.servlets.Login</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>EmployeeRegister</servlet-name>
		<servlet-class>com.revature.servlets.EmployeeRegister</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ClientAttendeeRegister</servlet-name>
		<servlet-class>com.revature.servlets.ClientAttendeeRegister</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>Home</servlet-name>
		<servlet-class>com.revature.servlets.Home</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>Employee</servlet-name>
		<servlet-class>com.revature.servlets.Employee</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>Wedding</servlet-name>
		<servlet-class>com.revature.servlets.Wedding</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>Attendee</servlet-name>
		<servlet-class>com.revature.servlets.Attendee</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>EmployeeRegister</servlet-name>
		<url-pattern>/employee-register</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ClientAttendeeRegister</servlet-name>
		<url-pattern>/client-attendee-register</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Employee</servlet-name>
		<url-pattern>/employee</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Wedding</servlet-name>
		<url-pattern>/wedding</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Attendee</servlet-name>
		<url-pattern>/attendee</url-pattern>
	</servlet-mapping>

</web-app>